[mysql]
socket = /tmp/mysql.sock
[mysqldump]
socket = /tmp/mysql.sock
[mysqladmin]
socket = /tmp/mysql.sock

[mysqld]
table_open_cache = 500
max_allowed_packet = 256M
socket = /tmp/mysql.sock

innodb_flush_log_at_trx_commit = 0
innodb_buffer_pool_size = 1G
tmp_table_size = 256M
max_heap_table_size = 256M
query_cache_type = ON
query_cache_size = 96M
max_connect_errors = 1000000
